{
    "name": "Starcraft2FrStreams",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Get the list of french streamers",
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["js/eventPage.js"],
        "persistent": false
    },
    "permissions": [
        "https://api.twitch.tv/*",
        "notifications",
        "storage",
        "alarms"
    ]
}
